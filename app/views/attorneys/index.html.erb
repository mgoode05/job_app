<p id="notice"><%= notice %></p>

<h1>Attorneys</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Barred states</th>
      <th>Current firm</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attorneys.each do |attorney| %>
      <tr>
        <td><%= attorney.name %></td>
        <td><%= attorney.barred_states %></td>
        <td><%= attorney.current_firm %></td>
        <td><%= link_to 'Show', attorney %></td>
        <td><%= link_to 'Edit', edit_attorney_path(attorney) %></td>
        <td><%= link_to 'Destroy', attorney, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attorney', new_attorney_path %>
